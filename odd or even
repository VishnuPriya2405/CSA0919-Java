import java.util.Scanner;
    public class Eoip
    {
        public static void main(String[] args)
        {
            Scanner reader=new Scanner(System.in);
            System.out.println("enter:");
            int n=reader.nextInt();
            if(n%2==0)
            {
                System.out.println("even");
            }
            else
            {
                System.out.println("odd");
            }
        }
    }

